{
    "name": "p7v/disable-floc-middleware",
    "description": "Disable Google's FLoC with help of PSR-15 middleware",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "~8.0.0 || ~8.1.0 || ~8.2.0",
        "psr/http-message": "^1.0 || ^2.0",
        "psr/http-server-handler": "^1.0",
        "psr/http-server-middleware": "^1.0"
    },
    "require-dev": {
        "nyholm/psr7": "^1.7.0",
        "phpunit/phpunit": "^9.6.7"
    },
    "authors": [
        {
            "name": "Alex Plekhanov",
            "email": "alex@p7v.app"
        }
    ],
    "provide": {
        "psr/http-server-middleware-implementation": "1.0"
    },
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "P7v\\DisableFloc\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
