{
    "name": "p7v/disable-floc-middleware",
    "description": "Disable Google's FLoC with help of PSR-15 middleware",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": ">=7.2.5",
        "psr/http-server-middleware": "^1.0"
    },
    "require-dev": {
        "nyholm/psr7": "^1.4",
        "phpunit/phpunit": "^8.5 | ^9.0",
        "symfony/polyfill-php73": "^1.23"
    },
    "authors": [
        {
            "name": "Alex Plekhanov",
            "email": "alex@p7v.app"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "P7v\\DisableFloc\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    }
}
